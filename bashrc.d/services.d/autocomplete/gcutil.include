# very simple one

DIR=$SAKURADIR/bashrc.d/services.d/autocomplete/

if [ ! -f $DIR/gcutil.list ] ; then
  echo "Building gcutil auto-completion styuff. Could take a few seconds... Thanks for your patience"
  cd $DIR &&
    make gcutil.list
fi

complete -W "`cat $DIR/gcutil.list`" gcutil
complete -W "`cat $DIR/gcutil.list`" gcutil-sakura
